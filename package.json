  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.4.4",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "format": "prettier --write src/**/*.js",
